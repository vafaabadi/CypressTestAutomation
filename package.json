{
  "name": "cypresstestautomation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "node_modules\\.bin\\cypress run",
    "headTest": "npm run test -- --headed --parallel",
    "chromeTest": "npm run test -- --browser chrome --parallel",
    "recordCloudTest": "npm run test -- --record --key a979428a-d58d-44e7-8a7b-954738a203b7",
    "cy:run": "cypress run --browser chrome --record --parallel --tag \"node-${NODE}\" --config \"pageLoadTimeout=100000,requestTimeout=30000,defaultCommandTimeout=30000\"",
    "cy:run:headed": "cypress run --headed",
    "cy:run:parallel": "cypress-parallel -s cy:run -t 6",
    "cy:run:parallel-headed": "cypress-parallel -s cy:run:headed -t 6",
    "cy:run_spec": "npx cypress run --spec 'cypress/integration/examples/BDD/*.feature' --headless --record --key a979428a-d58d-44e7-8a7b-954738a203b7 --parallel",
    "cy:run_browser": " npm run cy:run_spec -- --browser firefox --group Firefox --ci-build-id 1 & npm run cy:run_spec -- --browser chrome --group Chrome --ci-build-id 1 & npm run cy:run_spec -- --browser edge --group Edge --ci-build-id 1"
  },
  "cypress-cucumber-preprocessor": {
    "json": {
      "enabled": true,
      "output": "cypress/cucumberReports/cucumber-results.json"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@percy/cli": "^1.28.2",
    "@percy/cypress": "^3.1.2",
    "cypress": "^13.7.1",
    "cypress-iframe": "^1.0.1",
    "cypress-sql-server": "^1.0.0",
    "cypress-xpath": "^2.0.1",
    "multiple-cucumber-html-reporter": "^3.6.0",
    "neat-csv": "^5.1.0"
  },
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^20.0.3",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "convert-excel-to-json": "^1.7.0",
    "cypress-parallel": "^0.14.0"
  }
}
